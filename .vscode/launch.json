    "version": "0.2.0",
    "configurations": [
        {
            "name": "CLI: add placeholders",
            "type": "debugpy",
            "request": "launch",
            "module": "adjust.cli",
            "args": [
                "callbacks",
                "snapshot",
                "placeholders",
                "add",
                "-u",
                "invenio.sgn.com/v1/adjust",
                "-a",
                "!Cookie,!Bingo",
                "-n",
                "jamcity_ext",
            ],
            "console": "integratedTerminal",
        },
    ]
}
